#include <stdio.h>
#include "stm32l4xx.h"


int readPin(){
	int res = HAL_GPIO_ReadPin(GPIOA, GPIO_PIN_5);
	return res;
}


int detectSeq(){
	int state = 0;

	HAL_Delay(9);

	if (readPin()==1){
		return 1;
	}
	return 0;



	/*for (int i = 0; i<730000; i++){
		HAL_Delay(9)
		int value = readPin();
		//printf("Read value : %d\n\r", value);
		if (value!=state){
			return 0;
		}
	}
	return 1;*/
}
